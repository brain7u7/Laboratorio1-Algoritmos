#include <stdio.h>

int removeDuplicates(int* numero, int tamano) {
    if (tamano == 0) {
        return 0;
    }
    int posicion = 1; // Índice donde escribiremos el siguiente elemento único

    for (int i = 1; i < tamano; i++) {
        if (numero[i] != numero[i - 1]) {
            numero[posicion] = numero[i];
            posicion++;
        }
    }
    return posicion;
}

int main() {
    int numero[] = {1, 1, 2};
    int tamano = sizeof(numero) / sizeof(numero[0]);
    int k = removeDuplicates(numero, tamano);

printf("k = %d\n", k);
    printf("numero = [");
    for (int i = 0; i < k; i++) {
        printf("%d", numero[i]);
        if (i < k - 1) {
            printf(", ");
        }
    }
    printf("]\n");

    return 0;
}
